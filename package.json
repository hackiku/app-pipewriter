{
	"name": "pipewriter",
	"version": "0.0.1",
	"devDependencies": {
		"@sveltejs/adapter-vercel": "^5.5.2",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@types/sortablejs": "^1.15.8",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.5.0",
		"clsx": "^2.1.1",
		"concurrently": "^9.1.2",
		"firebase-admin": "^13.4.0",
		"formsnap": "^2.0.1",
		"lucide-svelte": "^0.511.0",
		"mdsvex": "^0.12.3",
		"paneforge": "^1.0.0-next.5",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"sveltekit-superforms": "^2.25.0",
		"tailwind-merge": "^3.3.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.0.0",
		"vaul-svelte": "^1.0.0-next.7",
		"vite": "^6.0.0",
		"wait-on": "^8.0.3",
		"zod": "^3.24.4"
	},
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"emulators": "firebase emulators:start",
		"emulators:ui": "firebase emulators:start --only hosting,firestore,auth,functions --import=./emulator-data",
		"emulators:export": "firebase emulators:export ./emulator-data",
		"emulators:clear": "firebase emulators:exec --only firestore 'echo Clearing data...'",
		"seed": "bun run scripts/seed/seed-elements.ts",
		"seed:elements": "bun run scripts/seed/seed-elements.ts",
		"seed:prompts": "bun run scripts/seed/better-prompts.ts",
		"seed:prod": "bun run scripts/seed-production.ts",
		"dev:fresh": "npm run emulators:clear && npm run seed:dev && npm run dev",
		"start:dev": "concurrently \"firebase emulators:start\" \"wait-on http://localhost:5000 && bun run seed:dev\""
	},
	"type": "module",
	"dependencies": {
		"@lucide/svelte": "^0.482.0",
		"firebase": "^11.7.3",
		"mode-watcher": "0.5.1",
		"sortablejs": "^1.15.6",
		"vanilla-picker": "^2.12.3"
	}
}
